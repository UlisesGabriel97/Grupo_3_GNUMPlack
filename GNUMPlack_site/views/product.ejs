<!DOCTYPE html>
<html lang="es">

    <%- include ("./partials/head") %> 

<body>
    <%- include ("./partials/header") %> 

    <main class="product">
        <h1>Productos</h1>
        <section>
            <% products.forEach(producto => { %>
                <article class="contenedor">
                    <h3><a href="/product/detail/<%= producto.id %> " target="_blank"><%= producto.name %> </a></h3>
                    <div>
                        <img class="placas" src="/img/<%- producto.image[0] %> " alt="placa de yeso" >
                    </div>
                </article>
             
            <% }) %>
           
        </section>
    </main>
<%- include ("./partials/footer") %> 
</body>

</html>