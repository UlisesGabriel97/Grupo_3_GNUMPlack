              <div class="description">
                  <p>descripcion del producto descripcion del producto descripcion del producto descripcion del producto descripcion del producto</p>
              </div>
              <div class="buy">
                  <div class="price">
                      <sup>$</sup>
                      <span>350</span>
                  </div>
                  <button class="btn">
                      <a href="">Comprar</a>
                  </button>
              </div>
          </div>
      </div>
    </div>
    <% }) %>
    </div>
    
  </div>

